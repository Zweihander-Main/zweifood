{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": ["error", "tab", { "SwitchCase": 1 }],
        // "linebreak-style": [
        //     "error",
        //     "unix"
        // ],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": ["error", { "allow": ["warn", "error", "debug"] }]
    }
}
