{
	"name": "ZweiFood",
	"version": "1.0.0",
	"description": "ZweiFood",
	"author": "ZweihÃ¤nder",
	"scripts": {
		"dev": "parcel app/index.html",
		"build": "parcel build app/index.html"
	},
	"main": "index.html",
	"private": true,
	"license": "MIT",
	"babel": {
		"presets": [
			"@babel/preset-env"
		]
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es6": true
		},
		"parserOptions": {
			"ecmaVersion": 2019,
			"sourceType": "module"
		},
		"extends": [
			"eslint:recommended",
			"plugin:prettier/recommended"
		],
		"rules": {
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"no-console": [
				"error",
				{
					"allow": [
						"warn",
						"error",
						"debug"
					]
				}
			],
			"no-mixed-spaces-and-tabs": [
				"error",
				"smart-tabs"
			]
		}
	},
	"jshintConfig": {
		"esversion": 9,
		"node": true,
		"globals": {
			"afterAll": false,
			"afterEach": false,
			"beforeAll": false,
			"beforeEach": false,
			"describe": false,
			"expect": false,
			"fit": false,
			"it": false,
			"jasmine": false,
			"jest": false,
			"pending": false,
			"pit": false,
			"require": false,
			"test": false,
			"xdescribe": false,
			"xit": false,
			"xtest": false
		},
		"laxbreak": true
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "es5",
		"tabWidth": 4,
		"useTabs": true,
		"arrowParens": "always"
	},
	"stylelint": {
		"extends": "stylelint-config-standard",
		"rules": {
			"indentation": "tab",
			"max-empty-lines": 2,
			"comment-whitespace-inside": null,
			"declaration-block-no-shorthand-property-overrides": true,
			"declaration-colon-newline-after": null,
			"no-descending-specificity": null
		}
	},
	"browserslist": [
		"> 0.2%",
		"last 2 versions",
		"Firefox ESR",
		"not dead",
		"maintained node versions"
	],
	"dependencies": {
		"bootstrap": "^3.3.7",
		"crypto-js": "^3.1.9-1",
		"fuzzyset": "0.0.4",
		"jquery": "^3.4.1",
		"jquery-migrate": "^3.1.0",
		"jquery-ui": "^1.12.1",
		"jquery.rateit": "^1.1.2",
		"knockout": "^3.5.0",
		"oauth-signature": "^1.5.0",
		"perfect-scrollbar": "^1.4.0",
		"popper.js": "^1.16.0",
		"vendor": "^1.7.0",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"@babel/core": "^7.4.0",
		"@babel/preset-env": "^7.4.0",
		"@babel/register": "^7.4.0",
		"eslint": "^5.15.3",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-prettier": "^3.0.1",
		"parcel-bundler": "^1.12.4",
		"prettier": "^1.16.4"
	}
}
